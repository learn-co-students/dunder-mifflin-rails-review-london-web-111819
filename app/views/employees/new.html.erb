<h1>Create a new Employee</h1>
<%= form_for @employee do |f| %>
    
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>

    <%= f.label :last_name %>
    <%= f.text_field :last_name %>

    <%= f.label :alias%>
    <%= f.text_field :alias%>

    <%= f.label :title%>
    <%= f.text_field :title%>

    <%= f.label :office %>
    <%= f.text_field :office %>

    <%= f.label :image_link %>
    <%= f.text_field :img_url %>

    <%= f.label :dog %>
    <%= f.collection_select :dog_id, Dog.all, :id, :name %>

    <%= f.submit %>

<% end %>